{
	"name": "AzureSqlDatabaseGenerique",
	"properties": {
		"description": "Permet de paramétrer la base de données",
		"parameters": {
			"SecretConnexionSql": {
				"type": "String",
				"defaultValue": "SqlDatabaseOutilsRedOpsConnectionString"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVaultLab01",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().SecretConnexionSql",
					"type": "Expression"
				}
			}
		}
	}
}